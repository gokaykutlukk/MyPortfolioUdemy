@model List<MyPortfolioUdemy.DAL.Entities.Skill>
<div class="col-six tab-full right">
    <h3>Teknolojiler</h3>

    <ul class="skill-bars">
        @foreach(var item in Model)
        {
        <li>
            <div class="progress percent "><span>@item.value</span></div>
            <strong>@item.Title</strong>
        </li>
        }
       
       
    </ul>
</div>